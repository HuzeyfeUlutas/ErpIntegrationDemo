{
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=personnel_access;Username=postgres;Password=postgres"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    }
  },
  "Elastic": {
    "Uris": [ "http://localhost:9200" ],
    "DataStream": {
      "Type": "logs",
      "Dataset": "pam",
      "Namespace": "api"
    }
  },
  "AllowedHosts": "*"
}
